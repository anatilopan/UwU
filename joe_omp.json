{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#B4BEFE",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue62a",
            "wsl": "\ue712"
          },
          "style": "plain",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} <#45475A>\ueee6 \ue0b2</>",
          "type": "os"
        },
        {
          "background": "#45475A",
          "foreground": "#89B4FA",
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " <#000000>\ue0bd </>",
            "home_icon": "\ueb06",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": " {{ .Path }} {{ if .Env.VIRTUAL_ENV }}‚ùÑ {{end}}",
          "type": "path"
        },
        {
          "background": "#585B70",
          "foreground": "#CDD6F4",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#45475A>\ue0b0 </>{{ .HEAD }}{{ if .Staging.Changed }}<#FF6F00> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} <#45475A>\ue0b2</>",
          "type": "git"
        },
        {
          "background": "#45475A",
          "foreground": "#CDD6F4",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": "<#45475A>\ue0b0</>{{ .FormattedMs }}s ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#45475A",
          "foreground": "#89B4FA",
          "leading_diamond": "\ue0c5",
          "style": "diamond",
          "template": " {{ if .SSHSession }}\ueba9 {{ .UserName }}<transparent> / </>{{ .HostName }}{{ end }}",
          "type": "session"
        },
        {
          "foreground": "#89B4FA",
          "properties": {
            "time_format": "15:04:05"
          },
          "template": "üïí{{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#89B4FA",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \uef12 ",
          "type": "status"
        },
        {
          "foreground": "#dd1e1e",
          "style": "plain",
          "template": " \u26a1",
          "type": "root"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
